class Student{    
    int id;    
    String name;    
    
    Student(){
	id =0;
	name="unknown"
	}    
}  
  
public class DefaultConstructor1{  
    public static void main(String args[]){    
        Student s1=new Student();    
        Student s2=new Student();    
   
        //s1.display();    
       // s2.display();    
    }    
} 